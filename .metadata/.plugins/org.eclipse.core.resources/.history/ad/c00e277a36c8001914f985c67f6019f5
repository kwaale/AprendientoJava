package bucles;

import java.util.Scanner;

public class Prueba {
	
	public static void main(String[] args) {
	Scanner s = new Scanner(System.in);
	System.out.println("dame un numero.");
	String num = s.next();
	
	

	if(num == "1")
	{
		System.out.println("Igual 1");
	}else
	{
		System.out.println("Diferente 1");
	}

}

}
