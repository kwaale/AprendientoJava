package primero;

public class Child {

//	3. Dada la clase "Parent" a continuación, cree una clase "Child"
	// que amplíe Parent, anule el método "calcularMultiplicacao" y sobrecargue el
	// método "concatenarFrases".

	public String concatenarFrases(String f1, String f2) {
		return f1 + f2;
	}

	public int calcularMultiplicacao(int fator1, int fator2) {
		return fator1 * fator2;
	}

}
