package bucles;

import java.util.Scanner;

public class Prueba {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		Scanner s = new Scanner(System.in);
		System.out.println("Clave");
		int q = s.nextInt();
		
		for (int i = 0; i < 4 && i > 0; i++) {
			int r = s.nextInt();
			System.out.println("Clave " + i + ":" + q + r);
		}

	}

}
