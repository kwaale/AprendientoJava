def cargaListaPalabras():
    listaPalabras = []
    for i in range(5):
        palabra = input("Introduce una palabra " + str(i + 1) + ": ")
        listaPalabras.append(palabra)
#    return listaPalabras
    print(listaPalabras)
def palabrasMasDe5caracteres(lista):
    for elemento in lista:
        if len(elemento) > 5:
            print("Mas de 5 caracteres: ", elemento)
    
cargaListaPalabras()
palabrasMasDe5caracteres(cargaListaPalabras())
